{
  "displayName": "Auto Reply Email System Dashboard",
  "gridLayout": {
    "columns": 12,
    "widgets": [
      {
        "title": "Email Processing Rate",
        "xyChart": {
          "dataSets": [
            {
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"logging.googleapis.com/user/email_processing_count\" resource.type=\"cloud_function\"",
                  "aggregation": {
                    "alignmentPeriod": "60s",
                    "perSeriesAligner": "ALIGN_RATE"
                  }
                },
                "unitOverride": "1"
              },
              "plotType": "LINE",
              "legendTemplate": "Emails Processed per Minute"
            }
          ],
          "timeshiftDuration": "0s",
          "yAxis": {
            "label": "Emails/min",
            "scale": "LINEAR"
          }
        }
      },
      {
        "title": "Response Time",
        "xyChart": {
          "dataSets": [
            {
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"logging.googleapis.com/user/email_response_time\" resource.type=\"cloud_function\"",
                  "aggregation": {
                    "alignmentPeriod": "60s",
                    "perSeriesAligner": "ALIGN_PERCENTILE_99"
                  }
                },
                "unitOverride": "s"
              },
              "plotType": "LINE",
              "legendTemplate": "p99 Response Time"
            },
            {
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"logging.googleapis.com/user/email_response_time\" resource.type=\"cloud_function\"",
                  "aggregation": {
                    "alignmentPeriod": "60s",
                    "perSeriesAligner": "ALIGN_PERCENTILE_50"
                  }
                },
                "unitOverride": "s"
              },
              "plotType": "LINE",
              "legendTemplate": "p50 Response Time"
            }
          ],
          "timeshiftDuration": "0s",
          "yAxis": {
            "label": "Seconds",
            "scale": "LINEAR"
          }
        }
      },
      {
        "title": "Function Executions",
        "xyChart": {
          "dataSets": [
            {
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"cloudfunctions.googleapis.com/function/execution_count\" resource.type=\"cloud_function\" resource.labels.function_name=\"process-email\"",
                  "aggregation": {
                    "alignmentPeriod": "60s",
                    "perSeriesAligner": "ALIGN_RATE"
                  }
                },
                "unitOverride": "1"
              },
              "plotType": "LINE",
              "legendTemplate": "Executions per Minute"
            }
          ],
          "timeshiftDuration": "0s",
          "yAxis": {
            "label": "Executions/min",
            "scale": "LINEAR"
          }
        }
      },
      {
        "title": "Function Execution Time",
        "xyChart": {
          "dataSets": [
            {
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"cloudfunctions.googleapis.com/function/execution_times\" resource.type=\"cloud_function\" resource.labels.function_name=\"process-email\"",
                  "aggregation": {
                    "alignmentPeriod": "60s",
                    "perSeriesAligner": "ALIGN_PERCENTILE_95"
                  }
                },
                "unitOverride": "ms"
              },
              "plotType": "LINE",
              "legendTemplate": "p95 Execution Time"
            },
            {
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"cloudfunctions.googleapis.com/function/execution_times\" resource.type=\"cloud_function\" resource.labels.function_name=\"process-email\"",
                  "aggregation": {
                    "alignmentPeriod": "60s",
                    "perSeriesAligner": "ALIGN_PERCENTILE_50"
                  }
                },
                "unitOverride": "ms"
              },
              "plotType": "LINE",
              "legendTemplate": "p50 Execution Time"
            }
          ],
          "timeshiftDuration": "0s",
          "yAxis": {
            "label": "Milliseconds",
            "scale": "LINEAR"
          }
        }
      },
      {
        "title": "Function Memory Usage",
        "xyChart": {
          "dataSets": [
            {
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"cloudfunctions.googleapis.com/function/user_memory_bytes\" resource.type=\"cloud_function\" resource.labels.function_name=\"process-email\"",
                  "aggregation": {
                    "alignmentPeriod": "60s",
                    "perSeriesAligner": "ALIGN_PERCENTILE_95"
                  }
                },
                "unitOverride": "By"
              },
              "plotType": "LINE",
              "legendTemplate": "p95 Memory Usage"
            }
          ],
          "timeshiftDuration": "0s",
          "yAxis": {
            "label": "Bytes",
            "scale": "LINEAR"
          }
        }
      },
      {
        "title": "Function Error Count",
        "xyChart": {
          "dataSets": [
            {
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"cloudfunctions.googleapis.com/function/execution_count\" resource.type=\"cloud_function\" resource.labels.function_name=\"process-email\" metric.labels.status=\"error\"",
                  "aggregation": {
                    "alignmentPeriod": "60s",
                    "perSeriesAligner": "ALIGN_SUM"
                  }
                },
                "unitOverride": "1"
              },
              "plotType": "STACKED_BAR",
              "legendTemplate": "Error Count"
            }
          ],
          "timeshiftDuration": "0s",
          "yAxis": {
            "label": "Count",
            "scale": "LINEAR"
          }
        }
      },
      {
        "title": "Pub/Sub Message Count",
        "xyChart": {
          "dataSets": [
            {
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"pubsub.googleapis.com/subscription/push_request_count\" resource.type=\"pubsub_subscription\" resource.labels.subscription_id=\"email-notifications-sub\"",
                  "aggregation": {
                    "alignmentPeriod": "60s",
                    "perSeriesAligner": "ALIGN_RATE"
                  }
                },
                "unitOverride": "1"
              },
              "plotType": "LINE",
              "legendTemplate": "Push Requests per Minute"
            }
          ],
          "timeshiftDuration": "0s",
          "yAxis": {
            "label": "Requests/min",
            "scale": "LINEAR"
          }
        }
      },
      {
        "title": "AI Model Token Usage",
        "xyChart": {
          "dataSets": [
            {
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"logging.googleapis.com/user/ai_token_usage\" resource.type=\"cloud_function\"",
                  "aggregation": {
                    "alignmentPeriod": "60s",
                    "perSeriesAligner": "ALIGN_SUM"
                  }
                },
                "unitOverride": "1"
              },
              "plotType": "LINE",
              "legendTemplate": "Tokens Used"
            }
          ],
          "timeshiftDuration": "0s",
          "yAxis": {
            "label": "Tokens",
            "scale": "LINEAR"
          }
        }
      },
      {
        "title": "Email Success Rate",
        "xyChart": {
          "dataSets": [
            {
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"logging.googleapis.com/user/email_success_rate\" resource.type=\"cloud_function\"",
                  "aggregation": {
                    "alignmentPeriod": "300s",
                    "perSeriesAligner": "ALIGN_MEAN"
                  }
                },
                "unitOverride": "%"
              },
              "plotType": "LINE",
              "legendTemplate": "Success Rate (%)"
            }
          ],
          "timeshiftDuration": "0s",
          "yAxis": {
            "label": "Percentage",
            "scale": "LINEAR"
          }
        }
      },
      {
        "title": "Function Instance Count",
        "xyChart": {
          "dataSets": [
            {
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"cloudfunctions.googleapis.com/function/instance_count\" resource.type=\"cloud_function\" resource.labels.function_name=\"process-email\"",
                  "aggregation": {
                    "alignmentPeriod": "60s",
                    "perSeriesAligner": "ALIGN_MEAN"
                  }
                },
                "unitOverride": "1"
              },
              "plotType": "STACKED_AREA",
              "legendTemplate": "Active Instances"
            }
          ],
          "timeshiftDuration": "0s",
          "yAxis": {
            "label": "Instances",
            "scale": "LINEAR"
          }
        }
      },
      {
        "title": "API Request Count",
        "xyChart": {
          "dataSets": [
            {
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"serviceruntime.googleapis.com/api/request_count\" resource.type=\"consumed_api\"",
                  "aggregation": {
                    "alignmentPeriod": "60s",
                    "perSeriesAligner": "ALIGN_RATE",
                    "crossSeriesReducer": "REDUCE_SUM",
                    "groupByFields": [
                      "metric.labels.service"
                    ]
                  }
                },
                "unitOverride": "1"
              },
              "plotType": "STACKED_BAR",
              "legendTemplate": "${metric.labels.service} - Requests/min"
            }
          ],
          "timeshiftDuration": "0s",
          "yAxis": {
            "label": "Requests/min",
            "scale": "LINEAR"
          }
        }
      },
      {
        "title": "System Health Status",
        "scorecard": {
          "timeSeriesQuery": {
            "timeSeriesFilter": {
              "filter": "metric.type=\"logging.googleapis.com/user/system_health\" resource.type=\"cloud_function\"",
              "aggregation": {
                "alignmentPeriod": "300s",
                "perSeriesAligner": "ALIGN_MEAN"
              }
            }
          },
          "thresholds": [
            {
              "value": 0.5,
              "color": "RED"
            },
            {
              "value": 0.9,
              "color": "YELLOW"
            }
          ],
          "sparkChartView": {
            "sparkChartType": "SPARK_LINE"
          }
        }
      }
    ]
  }
}
